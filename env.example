# Environment Configuration
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# Server Configuration for EC2
API_BASE_URL=http://<EC2 IP>:3000
ALLOWED_ORIGINS=http://<EC2 IP>:3000,http://localhost:3000

# AWS Configuration
AWS_REGION=ap-southeast-5
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key


BEDROCK_INFERENCE_PROFILE_ARN=Your_IPA

# S3 Configuration
S3_BUCKET_NAME=event-ai-storage
AWS_S3_BUCKET=event-ai-storage
AWS_S3_PUBLIC_URL=https://event-ai-storage.s3.ap-southeast-5.amazonaws.com

# Supabase Configuration
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# SageMaker Configuration
SAGEMAKER_ENDPOINT_NAME=event-ai-model-endpoint
USE_SAGEMAKER=true

# Forecast Model Configuration (New AI Model)
FORECAST_MODEL_ENDPOINT=http://56.68.21.46/forecast_inout

# Alternative AI Model API (Legacy, if not using SageMaker)
AI_MODEL_API_URL=https://your-ai-model-api.com/predict
AI_MODEL_API_KEY=your_ai_model_api_key

# Notification Configuration
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/whatsapp-alert
WHATSAPP_API_URL=https://api.whatsapp.com/send
WHATSAPP_API_KEY=your_whatsapp_api_key
NOTIFICATION_RECIPIENTS=+1234567890,+0987654321

# Security Configuration
JWT_SECRET=your_super_secret_jwt_key_here
JWT_EXPIRES_IN=24h
BCRYPT_SALT_ROUNDS=12

# Rate Limiting
RATE_LIMIT=100

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,https://your-frontend-domain.com

# Request Timeout (in milliseconds)
REQUEST_TIMEOUT=30000

# Application Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090

# Prediction Model Configuration
PREDICTION_MODEL_ENDPOINT=http://56.68.30.73/predict
PREDICTION_TIMEOUT=30000

# Cron Job Configuration
ENABLE_PREDICTION_CRON=true
# Run at standard 5-minute intervals: :00, :05, :10, :15, :20, :25, :30, :35, :40, :45, :50, :55
PREDICTION_CRON_PATTERN=0,5,10,15,20,25,30,35,40,45,50,55 * * * *
# Maximum timeframes to keep per gate (288 = 24 hours at 5-min intervals)
MAX_PREDICT_TIMEFRAMES=288
TZ=UTC

# CCTV Snapshot Configuration
ENABLE_CCTV_CRON=true
# Run every 3 minutes
CCTV_CRON_PATTERN=*/3 * * * *

# CCTV Image Quality Configuration
# Image format: 'jpeg' (smaller files) or 'png' (lossless, larger files)
CCTV_IMAGE_FORMAT=jpeg
# JPEG quality: 1-100 (100 = maximum quality, recommended for AI)
CCTV_JPEG_QUALITY=100
# PNG compression level: 0-9 (6 = default balance, 9 = max compression)
CCTV_PNG_COMPRESSION=6
# Minimum dimensions for AI model (images smaller will be upscaled)
CCTV_MIN_WIDTH=800
CCTV_MIN_HEIGHT=600
# Enable upscaling of low-resolution images
CCTV_UPSCALE_ENABLED=true
# Archive snapshots with timestamps (set to false to save storage)
CCTV_ARCHIVE_ENABLED=true

# CCTV Camera URLs (optional - can also be configured per event in database)
# Format: HTTP snapshot URL, RTSP stream, or file:// path for testing
# Leave empty if cameras are configured per event
CCTV_GATE_1_URL=
CCTV_GATE_A_URL=
CCTV_GATE_B_URL=
CCTV_GATE_C_URL=
CCTV_GATE_D_URL=

